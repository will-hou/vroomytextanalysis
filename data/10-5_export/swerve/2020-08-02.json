                                "type": "text",
                                "text": "const byte pinA = 2;\nconst byte pinB = 3; \/\/swap these pins if the encoder reads negative from what you want\n\nvolatile int encoderCount = 0; \/\/keep track of encoder position, could use long if you can't handle it overflowing often\nvolatile unsigned long lastEncoderCycleMicros = 0; \/\/when did the encoder last go through a cycle\nvolatile unsigned long lastEncoderCycleInterval = 0; \/\/how many microseconds did the last cycle of the encoder take\nvolatile boolean forwards = true; \/\/should speed be positive or negative (period between ticks doesn't tell direction)\n\nvoid setup() {\n  Serial.begin(250000);\n  pinMode(pinA, INPUT_PULLUP);\n  pinMode(pinB, INPUT_PULLUP);\n  attachInterrupt(digitalPinToInterrupt(pinA), pinAISR, CHANGE); \/\/pinAISR will be called each time channel A changes\n  attachInterrupt(digitalPinToInterrupt(pinB), pinBISR, CHANGE); \/\/pinBISR will be called each time channel B changes\n}\n\nvoid loop() {\n  Serial.print(encoderCount);\n  Serial.print(\",\");\n  Serial.print(calculateSpeed());\n  Serial.println();\n  delay(500);\/\/slow rate of printing, you can call calculateSpeed() as often as you want though\n}\nfloat calculateSpeed() {\n  if (lastEncoderCycleInterval == 0) { \/\/ avoid divide by zero before first encoder tick\n    return 0;\n  }\n\n\n  \/*\n     If the motor is turning really slowly or stopped,\n     it will be a long time between encoder ticks\n     so set speed to zero;\n  *\/\n  if (micros() - lastEncoderCycleMicros > 250000) {\n    return 0;\n  }\n\n  \/\/ calculate encoder ticks per second based on time between encoder cycles.\n  float speed = 1000000.0 \/ lastEncoderCycleInterval;\n  if (!forwards) {\n    speed = -speed;\n  }\n  return speed;\n}\n\nvoid pinAISR() {\n  lastEncoderCycleInterval = micros() - lastEncoderCycleMicros;\n  lastEncoderCycleMicros = micros();\n\n  if (digitalRead(pinA) == digitalRead(pinB)) {\n    encoderCount++;\n    forwards = true;\n  } else {\n    encoderCount--;\n    forwards = false;\n  }\n}\n\nvoid pinBISR() {\n  lastEncoderCycleInterval = micros() - lastEncoderCycleMicros;\n  lastEncoderCycleMicros = micros();\n\n  if (digitalRead(pinA) != digitalRead(pinB)) {\n    encoderCount++;\n    forwards = true;\n  } else {\n    encoderCount--;\n    forwards = false;\n  }\n}"
